{
  "name": "cixware/esewa-php-sdk",
  "version": "2.2.0",
  "type": "library",
  "description": "eSewa payment gateway integration in PHP.",
  "homepage": "https://developer.esewa.com.np",
  "license": "MIT",
  "authors": [
    {
      "name": "Cixware",
      "email": "support@cixware.io",
      "homepage": "https://cixware.io"
    }
  ],
  "keywords": [
    "api",
    "esewa",
    "fonepay",
    "gateway",
    "khalti",
    "nepal",
    "payment",
    "php"
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-openssl": "*",
    "ext-simplexml": "*",
    "guzzlehttp/guzzle": "^7.5",
    "vlucas/phpdotenv": "^5.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.11",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.14",
    "symfony/var-dumper": "^v5.4"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {},
  "autoload": {
    "psr-4": {
      "Cixware\\Payment\\Esewa\\": "src/Esewa"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
